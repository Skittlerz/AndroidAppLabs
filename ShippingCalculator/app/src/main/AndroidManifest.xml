<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.braun1792.shippingcalculator">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".appMenuActivity">
            <intent-filter>
            <category android:name="android.intent.category.LAUNCHER" />
            <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" android:label="@string/btnShipping"></activity>
        <activity android:name=".SecondActivity" android:label="@string/second_app_name"></activity>
        <activity android:name=".ThirdActivity" android:label="@string/calcApp"></activity>
        <activity android:name=".FourthActivity" android:label="@string/paintApp" android:screenOrientation="portrait"></activity>
        <activity android:name=".MyActivity" android:label="@string/personnelApp"></activity>
        <activity android:name=".PassingDataActivity" android:label="@string/passingData">
            <intent-filter >
                <action android:name="com.braun1792.PassingDataActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".PaintingCalculatorActivity" android:label="@string/paintCalc"></activity>
        <activity android:name=".PaintHelpActivity" android:label="@string/paintCalc"
            android:parentActivityName=".PaintingCalculatorActivity">
        <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".PaintingCalculatorActivity" />
        </activity>
        <activity android:name=".Shipping" android:label="@string/updatedShippingApp"></activity>

    </application>

</manifest>